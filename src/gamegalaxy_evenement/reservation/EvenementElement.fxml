<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="elementHBox" alignment="CENTER" cache="true" prefHeight="128.0" prefWidth="579.0" spacing="50.0" stylesheets="@caspian.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="113.0" prefWidth="109.0" spacing="10.0">
         <children>
            <Label fx:id="nomLabel" text="Nom" />
            <Label fx:id="dateLabel" text="Date" />
            <Label fx:id="dureeLabel" text="Durée" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="113.0" prefWidth="128.0" spacing="10.0">
         <children>
            <Label fx:id="capaciteLabel" text="Capacité" />
            <Label fx:id="descriptionLabel" text="Description" />
         </children>
      </VBox>
      <ImageView fx:id="imageView" fitHeight="113.0" fitWidth="123.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </ImageView>
      <VBox alignment="CENTER" prefHeight="113.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" text="Réserver" />
            <HBox blendMode="RED">
               <children>
                  <Label fx:id="errorMsg" text="ERROR_MSG" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" top="20.0" />
   </padding>
</HBox>
